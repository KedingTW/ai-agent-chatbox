# AWS CDK Configuration
CDK_DEFAULT_ACCOUNT=your-aws-account-id
CDK_DEFAULT_REGION=ap-northeast-1

# AWS Credentials (for browser use - VITE_ prefix required)
# Profile 1 (設定檔1) - Customer Assistant Agent 客戶助理
VITE_AWS_ACCESS_KEY_ID=your-access-key-id-here
VITE_AWS_SECRET_ACCESS_KEY=your-secret-access-key-here
VITE_AWS_SESSION_TOKEN=your-session-token-here

# Profile 2 (設定檔2) - Employee Assistant Agent 科定人助理
VITE_AWS_ACCESS_KEY_ID_B=your-access-key-id-b-here
VITE_AWS_SECRET_ACCESS_KEY_B=your-secret-access-key-b-here

# AWS Bedrock Agent Configuration (required for frontend)
VITE_AWS_REGION=ap-northeast-1

# Profile 1 Bedrock Configuration
VITE_AWS_BEDROCK_AGENT_ARN=your-bedrock-agent-arn-profile-1
VITE_AWS_BEDROCK_SESSION_ID=your-session-id-profile-1

# Profile 2 Bedrock Configuration
VITE_AWS_BEDROCK_AGENT_ARN_B=your-bedrock-agent-arn-profile-2
VITE_AWS_BEDROCK_SESSION_ID_B=your-session-id-profile-2
