# AWS CDK Configuration
CDK_DEFAULT_ACCOUNT=your-aws-account-id
CDK_DEFAULT_REGION=ap-northeast-1

# AWS Credentials (for browser use - VITE_ prefix required)
VITE_AWS_ACCESS_KEY_ID=your-access-key-id-here
VITE_AWS_SECRET_ACCESS_KEY=your-secret-access-key-here
VITE_AWS_SESSION_TOKEN=your-session-token-here

# AWS Bedrock Agent Configuration (required for frontend)
VITE_AWS_REGION=ap-northeast-1
VITE_AWS_BEDROCK_AGENT_ARN=your-bedrock-agent-arn
